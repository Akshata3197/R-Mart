<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <!--        <item name="colorPrimary">#F4F4F4</item>-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionMenuTextColor">@android:color/black</item>
        <item name="titleTextStyle">@style/MaterialAlertDialog.MaterialComponents.Title.Text</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:dropDownListViewStyle">@style/SpinnerStyle</item>
        <item name="android:windowSoftInputMode">stateHidden</item>
    </style>

    <style name="MyThemeOverlay_Toolbar2" parent="ThemeOverlay.MaterialComponents.Toolbar.Primary">
        <!-- This attributes is used by title -->
        <item name="android:textColorPrimary">@color/white</item>
    </style>

    <style name="TabNotSelected">
        <item name="android:layout_width">@dimen/button_curve</item>
        <item name="android:layout_height">@dimen/button_curve</item>
        <item name="android:padding">@dimen/_10sdp</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme2" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="button_color">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/submit</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="button_gray">
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/button_round_edges</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CustomLoadingDialogTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:bottomBright">@color/white</item>
        <item name="android:bottomDark">@color/white</item>
         <item name="android:bottomMedium">@color/white</item>
        <item name="android:centerBright">@color/white</item>
        <item name="android:centerDark">@color/white</item>
        <item name="android:centerMedium">@color/white</item>
        <item name="android:fullBright">@color/colorPrimary</item>
        <item name="android:fullDark">@color/colorPrimary</item>
        <item name="android:topBright">@color/colorPrimary</item>
        <item name="android:topDark">@color/colorPrimary</item>
    </style>

    <style name="AlertDialog" parent="Theme.MaterialComponents.Light.Dialog.Alert">
        <item name="android:textColor">#000000</item>
        <item name="android:textColorPrimary">#595959</item>
    </style>

    <style name="AppThemeNoActionBar" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionMenuTextColor">@style/CustomMenuTextAppearance</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:windowSoftInputMode">stateHidden</item>
    </style>
    <style name="CustomMenuTextAppearance" parent="@android:style/TextAppearance.Widget.IconMenu.Item">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="AuthenticationHeader">
        <item name="android:layout_marginBottom">40dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">26sp</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>

    <style name="item_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">20sp</item>
        <item name="android:padding">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/item_title_color</item>
    </style>
    <style name="product_preview_item_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/product_preview_title_color</item>
    </style>
    <style name="product_preview_item_value">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="item_product_offer">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/offer_color</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="product_unit_offer">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:textColor">@color/offer_color</item>
        <item name="android:layout_weight">2</item>
    </style>

    <style name="product_unit_in_active">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:textColor">#9F9F9F</item>
        <item name="android:text">In Active</item>
        <item name="android:layout_weight">2</item>
    </style>
    <style name="product_unit_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:textColor">#6E6E6E</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="product_unit_cost">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_weight">2</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="item_product_cost">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@color/item_title_color</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="item_product_unit">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="item_sub_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/item_sub_title_color</item>
    </style>

    <style name="upload_product_item_field">
        <item name="android:layout_height">@dimen/field_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">start|center</item>
        <item name="android:paddingLeft">@dimen/thumb_icon_padding</item>
        <item name="android:paddingRight">@dimen/thumb_icon_padding</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:background">@drawable/edit_product_field_bg</item>
        <item name="android:maxLines">1</item>
        <item name="android:minLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textSize">@dimen/text_regular_size</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="my_profile_tv" parent="upload_product_item_field">

    </style>
    <style name="list_items">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/option_select_item_bg</item>
        <item name="android:textColor">@color/black</item>
        <item name="hintTextColor">@color/edit_text_hint</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:minLines">1</item>
        <item name="android:gravity">center|left</item>
    </style>

    <style name="order_full_view_title">
        <item name="android:layout_marginTop">8dp</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_full_view_content">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_full_view_row">
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:padding">16dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="order_full_view_payment">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">@dimen/text_regular_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="item_order_product_key">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textColor">@color/item_sub_title_color</item>
    </style>

    <style name="item_order_product_value" parent="item_order_product_key">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="nv_item_view">
        <item name="android:padding">15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/nv_item_bg</item>
    </style>

    <style name="my_profile_title">
        <item name="android:padding">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:shadowColor">#00000033</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_regular_size</item>
    </style>

    <style name="text_style_medium_normal">
        <item name="android:textSize">@dimen/text_regular_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
    </style>

    <style name="text_style_medium_bold">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_style_extra_medium_normal">
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
    </style>

    <style name="text_style_extra_medium_bold">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_style_large_normal">
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
    </style>

    <style name="text_style_large_bold">
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_style_small">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
    </style>

    <style name="text_style_small_bold">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="RadioButton">
        <item name="android:minHeight">33dp</item>
        <item name="android:minWidth">70dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Theme_Custom_Dialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="colorAccent">#FFCC00</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="background">@color/white</item>
    </style>

    <style name="SpinnerStyle" parent="Widget.AppCompat.ListView.DropDown">
        <item name="android:divider">@color/black</item>
        <item name="android:dividerHeight">0.5dp</item>
    </style>
    <style name="mySpinnerTheme" parent="android:Theme.Holo.Dialog">
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@android:color/transparent</item>
        <item name="android:bottomDark">@android:color/transparent</item>
        <item name="android:bottomMedium">@android:color/transparent</item>
        <item name="android:centerBright">@android:color/transparent</item>
        <item name="android:centerDark">@android:color/transparent</item>
        <item name="android:centerMedium">@android:color/transparent</item>
        <item name="android:fullBright">@android:color/transparent</item>
        <item name="android:fullDark">@android:color/transparent</item>
        <item name="android:topBright">@android:color/transparent</item>
        <item name="android:topDark">@android:color/transparent</item>
    </style>
</resources>