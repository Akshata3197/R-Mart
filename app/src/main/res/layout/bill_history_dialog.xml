<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_10sdp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/date"
        android:layout_margin="@dimen/_10sdp"
        android:textColor="@color/black"
        android:id="@+id/date_title_textView"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:textColor="@color/black"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_toRightOf="@id/service_title_textView"
        android:id="@+id/dot_date_textView"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_toRightOf="@id/dot_date_textView"
        android:id="@+id/date_text"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/serice_type"
        android:layout_below="@id/date_title_textView"
        android:layout_margin="@dimen/_10sdp"
        android:textColor="@color/black"
        android:id="@+id/service_title_textView"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:textColor="@color/black"
        android:layout_below="@id/date_title_textView"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_toRightOf="@id/service_title_textView"
        android:id="@+id/dot_service_textView"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_below="@id/date_title_textView"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_toRightOf="@id/dot_date_textView"
        android:id="@+id/service_text"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/debit"
        android:layout_below="@id/service_title_textView"
        android:layout_margin="@dimen/_10sdp"
        android:textColor="@color/black"
        android:id="@+id/debit_title_textView"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:textColor="@color/black"
        android:layout_below="@id/service_title_textView"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_toRightOf="@id/service_title_textView"
        android:id="@+id/dot_debit_textView"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_below="@id/service_title_textView"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_toRightOf="@id/dot_date_textView"
        android:id="@+id/debit_text"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/credit"
        android:layout_below="@id/debit_title_textView"
        android:layout_margin="@dimen/_10sdp"
        android:textColor="@color/black"
        android:id="@+id/credit_title_textView"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:layout_below="@id/debit_title_textView"
        android:textColor="@color/black"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_toRightOf="@id/service_title_textView"
        android:id="@+id/dot_credit_textView"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_below="@id/debit_title_textView"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_toRightOf="@id/dot_date_textView"
        android:id="@+id/credit_text"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/balance"
        android:layout_below="@id/credit_title_textView"
        android:layout_margin="@dimen/_10sdp"
        android:textColor="@color/black"
        android:id="@+id/balance_title_textView"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=":"
        android:textColor="@color/black"
        android:layout_below="@id/credit_title_textView"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_toRightOf="@id/service_title_textView"
        android:id="@+id/balance_date_textView"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/credit_title_textView"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_toRightOf="@id/dot_date_textView"
        android:id="@+id/balance_text"/>

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="@string/ok"
        android:id="@+id/ok_textView"
        android:layout_below="@id/balance_title_textView"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:padding="@dimen/_10sdp"
        android:textColor="@color/white"
        android:layout_marginBottom="@dimen/_10sdp"
        android:background="@drawable/submit_button_bg"
        android:layout_marginTop="20dp"/>




</RelativeLayout>